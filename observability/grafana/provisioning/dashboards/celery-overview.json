{
  "uid": "celery-overview",
  "title": "Celery Overview - Marketing",
  "description": "Monitoramento de tasks Celery do FamaChat Marketing",
  "tags": ["marketing", "celery", "tasks"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "refresh": "30s",
  "schemaVersion": 39,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "Resumo",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "panels": []
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Task Throughput",
      "description": "Tasks processadas por segundo",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(celery_tasks_total[5m]))",
          "legendFormat": "tasks/sec",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 50, "color": "yellow" },
              { "value": 200, "color": "red" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Failure Rate",
      "description": "Taxa de falha das tasks (%)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(celery_tasks_total{status=\"failure\"}[5m])) / sum(rate(celery_tasks_total[5m])) * 100",
          "legendFormat": "Failure %",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 2, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          },
          "mappings": [],
          "noValue": "0%"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "auto"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Active Workers",
      "description": "Workers Celery ativos",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "celery_workers_active",
          "legendFormat": "Workers",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 1, "color": "yellow" },
              { "value": 2, "color": "green" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 101,
      "type": "row",
      "title": "Detalhamento",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "panels": []
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Tasks por Status",
      "description": "Taxa de tasks agrupadas por status (success, failure, retry)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(celery_tasks_total[5m])) by (status)",
          "legendFormat": "{{ status }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "stacking": { "mode": "none", "group": "A" },
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "success" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "failure" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "retry" },
            "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "sum"] }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Tasks por Fila",
      "description": "Taxa de tasks agrupadas por fila (default, ml, training)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(celery_tasks_total[5m])) by (queue)",
          "legendFormat": "{{ queue }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max", "sum"] }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Task Duration p95",
      "description": "Duracao percentil 95 por task",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(celery_task_duration_seconds_bucket[5m])) by (le, task_name))",
          "legendFormat": "{{ task_name }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 30, "color": "yellow" },
              { "value": 120, "color": "red" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 7,
      "type": "table",
      "title": "Top Tasks by Failure Rate",
      "description": "Top 10 tasks com maior taxa de falha na ultima hora",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum(rate(celery_tasks_total{status=\"failure\"}[1h])) by (task_name))",
          "legendFormat": "{{ task_name }}",
          "instant": true,
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 0.01, "color": "yellow" },
              { "value": 0.1, "color": "red" }
            ]
          },
          "mappings": [],
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "color-background", "mode": "gradient" },
            "inspect": false
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "task_name" },
            "properties": [
              { "id": "custom.width", "value": 350 },
              { "id": "displayName", "value": "Task" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [
              { "id": "displayName", "value": "Failures/sec" },
              { "id": "decimals", "value": 4 }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Time" },
            "properties": [
              { "id": "custom.hidden", "value": true }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Failures/sec" }],
        "footer": { "show": false }
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "__name__": true, "Time": true },
            "indexByName": {},
            "renameByName": {}
          }
        }
      ]
    }
  ]
}