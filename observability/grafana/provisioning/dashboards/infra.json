{
  "uid": "infra",
  "title": "Infrastructure - Marketing",
  "description": "Monitoramento de infraestrutura: Redis e PostgreSQL do FamaChat Marketing",
  "tags": ["marketing", "infra", "redis", "postgres"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "refresh": "30s",
  "schemaVersion": 39,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "templating": {
    "list": []
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "Status dos Servicos",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "panels": []
    },
    {
      "id": 1,
      "type": "stat",
      "title": "Redis Status",
      "description": "Status de conectividade do Redis",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "redis_up",
          "legendFormat": "Redis",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "DOWN", "color": "red", "index": 0 },
                "1": { "text": "UP", "color": "green", "index": 1 }
              }
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Postgres Status",
      "description": "Status de conectividade do PostgreSQL",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "pg_up",
          "legendFormat": "Postgres",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" },
              { "value": 1, "color": "green" }
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "DOWN", "color": "red", "index": 0 },
                "1": { "text": "UP", "color": "green", "index": 1 }
              }
            }
          ]
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 101,
      "type": "row",
      "title": "Redis",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "panels": []
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Redis Memory",
      "description": "Uso de memoria do Redis",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "redis_memory_used_bytes",
          "legendFormat": "Used Memory"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "thresholds" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "scheme",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" },
            "thresholdsStyle": { "mode": "area" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 268435456, "color": "yellow" },
              { "value": 536870912, "color": "red" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Redis Commands/sec",
      "description": "Comandos processados por segundo no Redis",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(redis_commands_processed_total[5m])",
          "legendFormat": "Commands/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Redis Connected Clients",
      "description": "Numero de clientes conectados ao Redis",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "redis_connected_clients",
          "legendFormat": "Connected Clients"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 50, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 102,
      "type": "row",
      "title": "PostgreSQL",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "panels": []
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Postgres Active Connections",
      "description": "Conexoes ativas no PostgreSQL",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "pg_stat_activity_count",
          "legendFormat": "Active Connections"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 50, "color": "yellow" },
              { "value": 90, "color": "red" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Postgres DB Size",
      "description": "Tamanho do banco de dados PostgreSQL",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "pg_database_size_bytes",
          "legendFormat": "{{ datname }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" },
              { "value": 1073741824, "color": "yellow" },
              { "value": 5368709120, "color": "red" }
            ]
          },
          "mappings": []
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Redis Keyspace Hits vs Misses",
      "description": "Taxa de hits e misses no keyspace do Redis",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(redis_keyspace_hits_total[5m])",
          "legendFormat": "Hits/sec"
        },
        {
          "refId": "B",
          "expr": "rate(redis_keyspace_misses_total[5m])",
          "legendFormat": "Misses/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "gradientMode": "none",
            "showPoints": "never",
            "spanNulls": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "scaleDistribution": { "type": "linear" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Hits/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Misses/sec" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] }
      }
    }
  ]
}